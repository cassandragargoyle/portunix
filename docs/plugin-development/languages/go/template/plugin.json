{
  "name": "go-plugin-template",
  "version": "1.0.0",
  "description": "Template for Go-based Portunix plugins",
  "author": "Your Name",
  "license": "MIT",
  "plugin": {
    "type": "grpc",
    "binary": "./plugin",
    "runtime": "native",
    "port": 9001,
    "health_check_interval": 30000000000
  },
  "dependencies": {
    "portunix_min_version": "1.10.0",
    "os_support": ["linux", "windows", "darwin"]
  },
  "ai_integration": {
    "mcp_tools": [
      {
        "name": "example_tool",
        "description": "Example tool for demonstration"
      }
    ]
  },
  "permissions": {
    "filesystem": ["read"],
    "network": ["outbound"],
    "database": [],
    "system": [],
    "level": "limited"
  },
  "commands": [
    {
      "name": "help",
      "description": "Show help for plugin",
      "subcommands": [],
      "parameters": [],
      "examples": ["portunix plugin-name help"]
    }
  ]
}
